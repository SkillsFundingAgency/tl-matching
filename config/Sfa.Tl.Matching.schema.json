{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {},
  "id": "http://example.com/example.json",
  "properties": {
    "Authentication": {
      "properties": {
        "WtRealm": {
          "minLength": 1,
          "type": "string",
          "environmentVariable": "AuthenticationWtRealm"
        },
        "MetaDataEndpoint": {
          "minLength": 1,
          "type": "string",
          "environmentVariable": "AuthenticationMetaDataEndpoint"
        }
      },
      "additionalProperties": false,
      "required": [
        "WtRealm",
        "MetaDataEndpoint"
      ],
      "type": "object"
    },
    "AuthorisedAdminUserEmail": {
      "type": "string",
      "environmentVariable": "AuthorisedAdminUserEmail"
    },
    "AzureSearchConfiguration": {
      "properties": {
        "Name": {
          "minLength": 1,
          "type": "string",
          "environmentVariable": "AzureSearchName"
        },
        "AdminApiKey": {
          "minLength": 1,
          "type": "string",
          "environmentVariable": "AzureSearchAdminApiKey"
        },
        "QueryApiKey": {
          "minLength": 1,
          "type": "string",
          "environmentVariable": "AzureSearchQueryApiKey"
        }
      },
      "additionalProperties": false,
      "required": [
        "Name",
        "AdminApiKey",
        "QueryApiKey"
      ],
      "type": "object"
    },
    "PostCodeRetrieverBaseUrl": {
      "minLength": 1,
      "type": "string",
      "environmentVariable": "PostCodeRetrieverBaseUrl"
    },
    "GovNotifyApiKey": {
      "type": "string",
      "environmentVariable": "GovNotifyAPIKey"
    },
    "EmailDeliveryStatusToken": {
      "type": "string",
      "environmentVariable": "EmailDeliveryStatusToken"
    },
    "SendEmailEnabled": {
      "type": "boolean",
      "environmentVariable": "SendEmailEnabled"
    },
    "GoogleMapsApiBaseUrl": {
      "minLength": 1,
      "type": "string",
      "environmentVariable": "GoogleMapsApiBaseUrl"
    },
    "GoogleMapsApiKey": {
      "minLength": 1,
      "type": "string",
      "environmentVariable": "GoogleMapsApiKey"
    },
    "CalendarJsonUrl": {
      "minLength": 1,
      "type": "string",
      "environmentVariable": "CalendarJsonUrl"
    },
    "EmployerFeedbackWorkingDayOfMonth": {
      "type": "integer",
      "default": 10,
      "environmentVariable": "EmployerFeedbackWorkingDayOfMonth"
    },
    "ProviderFeedbackWorkingDayOfMonth": {
      "type": "integer",
      "default": 10,
      "environmentVariable": "ProviderFeedbackWorkingDayOfMonth"
    },
    "BlobStorageConnectionString": {
      "type": "string",
      "environmentVariable": "BlobStorageConnectionString"
    },
    "ServiceBusConnectionString": {
      "type": "string",
      "environmentVariable": "ServiceBusConnectionString"
    },
    "SessionRedisConnectionString": {
      "minLength": 1,
      "type": "string",
      "environmentVariable": "SessionRedisConnectionString"
    },
    "SqlConnectionString": {
      "minLength": 1,
      "type": "string",
      "environmentVariable": "SqlConnectionString"
    },
    "SupportInboxEmail": {
      "minLength": 1,
      "type": "string",
      "environmentVariable": "SupportInboxEmail"
    },
    "MatchingServiceSupportEmailAddress": {
      "minLength": 1,
      "type": "string",
      "environmentVariable": "MatchingServiceSupportEmailAddress"
    }
  },
  "additionalProperties": false,
  "required": [
    "Authentication",
    "SessionRedisConnectionString",
    "SqlConnectionString"
  ],
  "type": "object"
}